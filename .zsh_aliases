alias tree='tree -A'
alias LS='ls'
alias sa='svn add'
alias sr='svn revert --depth=infinity'
alias sd='svn diff --diff-cmd=meld'
alias sup='svn update'
alias sst='svn st -qu'
alias sll='svn log -l'
alias sco='svn co'
alias sci='svn ci -m'
alias si='svn info'
alias ssx='svn propset svn:executable on'
alias sdx='svn propdel svn:executable'
